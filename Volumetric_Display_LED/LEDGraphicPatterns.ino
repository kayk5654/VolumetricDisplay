void LEDsetup() {
  for (int i = 0; i < 8; i++) {
    pinMode(LEDrow[i], OUTPUT);
    digitalWrite(LEDrow[i], HIGH);
    pinMode(LEDcol[i], OUTPUT);
    digitalWrite(LEDcol[i], LOW);
  }
}

void LEDcontrol(char command[8][8]) {
  /*
    for (int row = 0; row < 8; row++) {
    for (int col = 0; col < 8; col++) {
      if (command[row][col] == 0) {
        //turn off
        digitalWrite(LEDrow[row],HIGH);
        digitalWrite(LEDcol[col],LOW);
      } else {
        //turn on
        digitalWrite(LEDrow[row],LOW);
        digitalWrite(LEDcol[col],HIGH);
      }
    }
    }
  */
  // カソードのループ
  for ( int row = 0; row < 8; row++ )
  {
    digitalWrite( LEDrow[row], LOW ); // LOWに変更
    // アノードのループ
    for ( int col = 0; col < 8; col++ )
    {
      digitalWrite( LEDcol[col], command[row][col] );  // HIGH or LOW
    }
    delayMicroseconds(100); //点灯時間
    for ( int col = 0; col < 8; col++ )
    {
      digitalWrite( LEDcol[col], LOW ); // LOWに戻す
    }
    digitalWrite( LEDrow[row], HIGH );  // HIGHに戻す
  }

}

